using System;

namespace sort
{
    class Program
    {
        static void Main(string[] args)
        {
            string A = Console.ReadLine();
            
            for (int i = 0; i < A.Length; i++)
            {
                if (A[i] >= '0' && A[i] <= '9')
                {
                    throw new Exception ("в строке есть цифры");
                }
            }
        }
    }
}
